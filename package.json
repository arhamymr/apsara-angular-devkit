{
  "name": "apsara-angular-devkit",
  "version": "1.0.0",
  "description": "shadcn-style UI component library and CLI for Angular",
  "private": true,
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "packages/ui",
    "packages/cli",
    "packages/boilerplate/*"
  ],
  "scripts": {
    "build:cli": "cd packages/cli && npm run build",
    "build:ui": "cd packages/ui && npm run build",
    "build": "npm run build:cli && npm run build:ui"
  },
  "keywords": [
    "angular",
    "ui",
    "components",
    "tailwind",
    "shadcn",
    "cli"
  ],
  "license": "MIT"
}
